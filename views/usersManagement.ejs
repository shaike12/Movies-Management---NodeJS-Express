<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Users Management</title>
  </head>
  <body>
    <a href="/users_management/edit">Add New</a>
    <ul>
      <% users.map(user => { %>
      <li><%=user.Username%></li>
      <a href="<%= `/users_management/delete/${user.id}` %>">Delete</a>
      <a href="<%= `/users_management/edit/${user.id}` %>">Edit</a>
      <% }) %>
    </ul>
    <input type="button" value="Back" onclick="history.back(-1)" />
  </body>
</html>
